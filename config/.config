
#
# Dataset Configuration
#
CONFIG_DATASET_PATH="/workspace/financebench"
CONFIG_DATASET_PDF_DIR="/workspace/financebench/pdfs"
CONFIG_FILTER_NUMERICAL=y
CONFIG_MAX_SAMPLES=1
# CONFIG_USE_SAMPLE_INDICES is not set
# CONFIG_USE_SAMPLE_RANGE is not set
# end of Dataset Configuration

#
# Model Configuration
#

#
# Local Model (On-Device)
#
CONFIG_LOCAL_MODEL_NAME="meta-llama/Llama-3.1-8B-Instruct"
CONFIG_LOCAL_TEMPERATURE=20
CONFIG_LOCAL_NUM_CTX=4096
CONFIG_LOCAL_BACKEND_SGLANG=y
CONFIG_SGLANG_BASE_URL="http://localhost:8000/v1"
CONFIG_LOGIT_PROCESSOR_PATH=""
# end of Local Model (On-Device)

#
# Remote Model (Cloud)
#
CONFIG_REMOTE_MODEL_NAME="gpt-4o"
CONFIG_REMOTE_TEMPERATURE=0
# end of Remote Model (Cloud)
# end of Model Configuration

#
# Protocol Configuration
#

#
# Select protocols to evaluate
#
CONFIG_PROTOCOL_MINIONS=y
# CONFIG_PROTOCOL_MINION is not set
# CONFIG_PROTOCOL_REMOTE_ONLY is not set
# CONFIG_PROTOCOL_LOCAL_ONLY is not set

#
# --- Protocol Settings ---
#

#
# Common Protocol Settings
#
CONFIG_MAX_ROUNDS=4
CONFIG_NUM_SAMPLES_PER_TASK=1
# end of Common Protocol Settings

#
# MINIONS Settings
#
CONFIG_MINIONS_TASKS_PER_ROUND=12
# CONFIG_MINIONS_CHUNK_BY_SECTION is not set
# CONFIG_MINIONS_CHUNK_BY_PAGE is not set
CONFIG_MINIONS_CHUNK_MULTIPLE_PAGES=y
CONFIG_MINIONS_PAGES_PER_CHUNK=5
# CONFIG_MINIONS_CHUNK_BY_PARAGRAPH is not set
# CONFIG_MINIONS_CHUNK_HIERARCHICAL is not set
CONFIG_MINIONS_MAX_CHUNK_SIZE=3000
CONFIG_MINIONS_MAX_JOBS_PER_ROUND=0
CONFIG_MINIONS_NO_RETRIEVAL=y
# CONFIG_MINIONS_RETRIEVAL_BM25 is not set
# CONFIG_MINIONS_RETRIEVAL_EMBEDDING is not set
# end of MINIONS Settings
# end of Protocol Configuration

#
# Global Settings
#
CONFIG_OUTPUT_DIR="evaluate/results"
# CONFIG_SKIP_ACCURACY is not set
CONFIG_USE_CACHE=y
CONFIG_CACHE_PREFIX=""
CONFIG_PROMPT_SET=""
# end of Global Settings
